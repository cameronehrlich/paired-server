<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

  <% include ../partials/nav.ejs %>

  <div class="jumbotron text-center">
    <h1>Paired Admin</h1>
  </div>
  <div>
    <h1>Users</h1>
    <table class="table table-striped">
      <% for (var i = 0; i < users.length; i++) { %>
      <tr>
        <td>
          <%= users[i].get('email') %>
        </td>
        <td>
          <%= users[i].get('username') %>
        </td>
      </tr>
      <% } %>
    </table>
  </div>

</body>
</html>
